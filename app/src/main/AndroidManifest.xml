    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo1"
        android:label="Save Life"
        android:roundIcon="@mipmap/logo1"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SignupActivity" />
        <activity android:name=".TempActivity" />
        <activity android:name=".SignupContActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ProfileActivity"
            android:label="@string/title_activity_profile"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".PersonActivity" />
        <activity android:name=".GroupsActivity" />
        <activity android:name=".ListActivity" />
        <activity android:name=".ContactActivity" />
        <activity android:name=".HelpActivity" />
        <activity android:name=".DisclaimerActivity" />
        <activity android:name=".UpdateActivity" />
        <activity android:name=".ForgotActivity"></activity>
    </application>

</manifest>